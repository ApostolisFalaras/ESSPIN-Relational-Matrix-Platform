                    <!-- Icon representing inequalities, through an unevenly split pie chart -->
                    
                    <img alt="Checkmark Logo">

                    <h3>Detailed Results</h3>

                    <p>
                        Detailed findings for the <b><%= sourceOfResults %></b> are listed below.
                        Use the cards to explore specific impacts and data.
                    </p>

                    
                    <div class="results-flex">
                        <iframe name="hidden-frame" style="display:none;"></iframe>

                        <% if (locals.results) { %>

                            <% results.forEach((res,index) => { %>
                                <% if (res.table === "Estimated Inputs") { %>

                                    <!-- Results Card for an Estimated Inputs Finding -->
                                    <div class="result-card <%= res.effect_for_color %>">
                                        
                                        <!-- Results Card Header -->
                                        <form action="/bookmark-result-card" method="POST" target="hidden-frame"> 
                                            <input type="hidden" name="resultCardId" value="<%= res.id %>" id="result-card-id">
                                            <input type="hidden" name="resultCardTable" value="<%= res.table %>" id="result-card-table">

                                            <div class="bookmark-wrapper <%= (res.isBookmarked === "true") ? 'selected' : '' %>">
                                                <div class="bookmark <%= (res.isBookmarked === "true") ? 'selected' : '' %> <%= res.effect_for_color %>"></div>
                                            </div>
                                        </form>
                                        
                                        <h3 class="<%= res.effect_for_color %>"><%= index+1 %>. <%= res.table %></h3>
                                        
                                        <dl>
                                            <!-- Variables Subsection in the Results Card -->
                                            <dt>Level of Analysis</dt>
                                            <dd><%=res.level_of_analysis%></dd>
                                            <% if (res.level_of_analysis === "Other") { %>
                                                <dt>Other Level of Analysis</dt>
                                                <dd><%= res.level_of_analysis_other %></dd>
                                            <% } %>
                                            
                                            <dt>Dependent Variable</dt>
                                            <dd><%=res.selection_dependent%></dd>
                                            <% if (res.selection_dependent.startsWith("Other")) { %>
                                                <dt>Other Dependent Variable</dt>
                                                <dd><%=res.other_dependent%></dd>
                                            <% } %>

                                            <dt>Independent Variable</dt>
                                            <dd><%=res.selection_independent%></dd>
                                            <% if (res.selection_independent.startsWith("Other")) { %>
                                                <dt>Other Independent Variable</dt>
                                                <dd><%=res.other_independent%></dd>
                                            <% } %>

                                            <!-- Variables Relationship Subsection in the Results Card -->
                                            <dt>Type</dt>
                                            <dd>Causal</dd>
                                            
                                            <dt>Impact</dt>
                                            <dd><%=res.effect_direction%></dd>

                                            <% if (res.variable_condition) { %>
                                                <dt>Conditional Variable</dt>
                                                <dd><%=res.variable_condition%></dd>

                                                <% if (res.variable_condition.startsWith("Other")) { %>
                                                    <dt>Other Conditional Variable</dt>
                                                    <dd><%=res.other_condition%></dd>
                                                <% } %>
                                                <dt>Conditional Effect</dt>
                                                <dd><%=res.type_of_condition_effect%></dd>
                                            <% } %>

                                            <!-- Additional Details about the current Finding -->
                                            <dt>Years</dt>
                                            <dd><%=res.start_year%> - <%=res.end_year%></dd>
                                            <dt>Countries</dt>
                                            <dd>Europe</dd>
                                        </dl>

