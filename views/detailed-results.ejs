                    <% } else if (sourceOfResults === "Correlations Table") { %>
                        <div class="color-case"> <div class="color-shape positive-color"></div><span class="color-label">Both Positive</span></div>
                        <div class="color-case"> <div class="color-shape rather-positive-color"></div><span class="color-label">One Positive, One No-Effect</span></div>
                        <div class="color-case"> <div class="color-shape negative-color"></div><span class="color-label">Both Negative</span></div>
                        <div class="color-case"> <div class="color-shape rather-negative-color"></div><span class="color-label">One Negative, One No-Effect</span></div>
                        <div class="color-case"> <div class="color-shape inconclusive-color"></div><span class="color-label">One Positive, One Negative</span></div>
                        <div class="color-case"> <div class="color-shape no-effect-color"></div><span class="color-label">Both No-Effect</span></div>

                    <% } else if (sourceOfResults === "Granger Causalities Table") { %>
                        <div class="color-case"> <div class="color-shape positive-color"></div><span class="color-label">Consistent Positive</span></div>
                        <div class="color-case"> <div class="color-shape rather-positive-color"></div><span class="color-label">Predominantly Positive</span></div>
                        <div class="color-case"> <div class="color-shape negative-color"></div><span class="color-label">Consistent Negative</span></div>
                        <div class="color-case"> <div class="color-shape rather-negative-color"></div><span class="color-label">Predominantly Negative</span></div>
                        <div class="color-case"> <div class="color-shape inconclusive-color"></div><span class="color-label">Mixed Trend</span></div>
                    <% } %>
                </div>
            </div>

